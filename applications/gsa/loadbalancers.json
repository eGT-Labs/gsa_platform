{
	"name": "fbopen",
	"region": "us-east-1",
	"ingress_rules":[
		{
			"port":80,
				"hosts":[
				  "0.0.0.0/0"
				]
		}
     ],
	"healthcheck": {
		"target":"HTTP:80/",
		"unhealthy-threshold":5
		},
		"listeners": [
			{
					"lb-port": 80,
					"lb-protocol": "HTTP",
					"instance-port": 80,
					"instance-protocol": "HTTP"
			}
	    ]
}